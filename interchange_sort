#include<iostream>
using namespace std;
void nhapmang(int a[],int n);
void interchangeSort(int a[],int n);
void nhapmang(int a[], int n);

void xuatmang(int a[], int n);
int main()
{
	int a[100];
	int n;
	cout << "nhap n:";
	cin >> n;
	nhapmang(a, n);
	interchangeSort(a, n);
	xuatmang(a, n);

	return 0;
}
void nhapmang(int a[],int n)
{
	for (int i = 0; i < n; i++)
	{
		cout << "nhap mang";
		cin >> a[i];
	}

}
void interchangeSort(int a[], int n)
{
	for (int i = 0; i < n - 1; i++)
	{
		for (int j = i + 1; j < n; j++)
		{
			if (a[j] < a[i])
			{
				swap(a[j], a[i]);
			}
		}

	}
}

void xuatmang(int a[], int n)
{
	for (int i = 0; i < n ; i++)
	{
		cout<< a[i]<<" ";
	}

}
